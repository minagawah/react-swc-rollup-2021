{
  "name": "react-swc-rollup-2021",
  "version": "0.1.0",
  "private": false,
  "description": "A template project for React apps using SWC and Rollup.",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier --write \"*.*\" \"src/**/*.{css,html,js,jsx}\"",
    "pretty": "pretty-quick",
    "watch": "rollup -c -w",
    "serve": "npx serve -p 8080 public",
    "start": "concurrently --kill-others-on-fail \"$npm_execpath run serve\" \"$npm_execpath run watch\"",
    "build": "rollup -c"
  },
  "repository": "https://github.com/minagawah/react-swc-rollup-2021.git",
  "keywords": [
    "React",
    "Rollup",
    "SWC"
  ],
  "author": "Hiroki Minagawa <minagawa@tokyo800.jp>",
  "license": "(Unlicense OR MIT)",
  "bugs": {
    "url": "https://github.com/minagawah/react-swc-rollup-2021/issues"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@swc/cli": "^0.1.36",
    "@swc/core": "^1.2.50",
    "@testing-library/react": "^11.2.5",
    "concurrently": "^6.0.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-swc": "^0.1.4",
    "rollup-plugin-terser": "^7.0.2"
  }
}
